{
  "name": "ReadAid",
  "description": "Show / Hide: '/', --------------------------------------------- Width: (reduce) '[' | (expand) ']', ---------------------- Height: (reduce) '-' | (expand) '+'",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["readAid.js"],
      "css": ["readAid.css"],
      "run_at": "document_end"
    }
  ]
}

